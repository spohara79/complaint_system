{
    "monitored_mailboxes": [
      "complaints1@yourdomain.com",
      "support_inbox@yourdomain.com"
    ],
    "distribution_list_email": "complaints_handling_team@yourdomain.com",
    "client_id": "YOUR_AZURE_APP_CLIENT_ID",
