    "redirect_uri": "http://localhost",
    "authority": "https://login.microsoftonline.com/YOUR_AZURE_TENANT_ID",
    "scopes": [
      "https://graph.microsoft.com/Mail.ReadWrite",
      "https://graph.microsoft.com/Mail.Send",
