    ],
    "client_secret": "YOUR_AZURE_APP_CLIENT_SECRET",
    "exclusions": {
      "from": [
        "no-reply@*",
        "automated-system@.*\\.example\\.com"
      ],
      "subject": [
        "Out of Office",
        "Delivery Status Notification"
      ]
    },
    "cert_path": "/path/to/cert.pem",
    "private_key_path": "/path/to/private.key",
    "cert_thumbprint": "CERT_THUMBPRINT"
